{
    ".projections.json": {
        "type": "Projections"
    },
    ".gitignore": {
        "type": "GitIgnore"
    },
    "README.md": {
        "type": "Readme"
    },
    "LICENSE": {
        "type": "License"
    },
    ".travis.yml": {
        "type": "Travis"
    },
    "install.sh": {
        "type": "Install"
    },
    "bin/*": {
        "type": "Bins"
    },
    "dotconfigs/*": {
        "type": "Dotconfigs"
    },
    "systemd/*.service": {
        "type": "Services",
        "alternate": "systemd/{}.timer"
    },
    "systemd/*.timer": {
        "type": "Timers",
        "alternate": "systemd/{}.service"
    },
    "system/*": {
        "type": "System"
    },
    "git/bin/*": {
        "type": "GitBins"
    },
    "git/templates/*": {
        "type": "GitTemplates"
    },
    "git/templates/hooks/*": {
        "type": "GitHooks"
    },
    "shell/alias/*": {
        "type": "Alias"
    },
    "shell/init/*": {
        "type": "ShellInit"
    },
    "shell/host/*": {
        "type": "Host"
    },
    "shell/samples/*": {
        "type": "ShellSamples"
    },
    "shell/settings/*": {
        "type": "ShellSettings"
    },
    "shell/banner": {
        "type": "Banner"
    }
}
